      <articlesItem id="1155" key="d4f29da7-7ad9-41e6-91df-82917ab7d3f8" parentID="1078" level="3" creatorID="0" sortOrder="3" createDate="2018-09-19T20:03:22" updateDate="2018-10-10T16:56:56" nodeName="JavaScript Objects Quickly Explained" urlName="javascript-objects-quickly-explained" path="-1,1074,1078,1155" isDoc="" nodeType="1061" creatorName="Mariann" writerName="Mariann" writerID="0" template="1060" nodeTypeAlias="articlesItem" isPublished="true">
        <metaDescription><![CDATA[A short explaining of JavaScript Objects]]></metaDescription>
        <metaKeywords><![CDATA[Javascript, Objects, Arrays]]></metaKeywords>
