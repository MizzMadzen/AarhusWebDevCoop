      <articlesItem id="1104" key="8590c81d-31a3-4000-a690-987cdc35f497" parentID="1078" level="3" creatorID="0" sortOrder="1" createDate="2018-09-17T20:27:56" updateDate="2018-10-10T16:55:05" nodeName="13 Skills You'll Need to be a Front End Developer" urlName="13-skills-youll-need-to-be-a-front-end-developer" path="-1,1074,1078,1104" isDoc="" nodeType="1061" creatorName="Mariann" writerName="Mariann" writerID="0" template="1060" nodeTypeAlias="articlesItem" isPublished="true">
